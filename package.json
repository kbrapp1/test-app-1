{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "./lib/monitoring/**",
    "./app/globals.css"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:fast": "next dev --turbopack --port 3000",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:webpack": "next dev",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "lint:watch": "nodemon --watch \"**/*.{ts,tsx,js,jsx}\" --ext ts,tsx,js,jsx --exec \"npm run lint\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "typecheck:strict": "tsc --noEmit --project tsconfig.strict.json",
    "typecheck:strict:watch": "tsc --noEmit --watch --project tsconfig.strict.json",
    "quality:check": "npm run typecheck && npm run lint",
    "quality:watch": "concurrently \"npm run typecheck:watch\" \"npm run lint:watch\"",
    "dev:quality": "node scripts/dev-quality-check.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true NODE_ENV=production npm run build",
    "analyze:turbo": "ANALYZE=true NODE_ENV=production npm run build:turbo",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=lighthouse-report.html --no-enable-error-reporting && start lighthouse-report.html",
    "perf": "powershell -ExecutionPolicy Bypass -File scripts/performance-analysis.ps1",
    "perf:turbo": "powershell -ExecutionPolicy Bypass -File scripts/turbopack-analysis.ps1"
  },
