{
  "mcpServers": {
      "supabase": {
          "command": "cmd",
          "args": [
              "/c",
              "npx",
              "-y",
              "@supabase/mcp-server-supabase@latest",
              "--access-token",
              "{arg}"          ]
      },
      "context7": {
        "command": "npx",
        "args": ["-y", "@upstash/context7-mcp@latest"]
      },
      "playwright": {
        "command": "npx",
        "args": [
          "@playwright/mcp@latest"
          ]
      }
  }
}